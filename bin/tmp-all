git checkout master
rm -fR /tmp/master
mkdir /tmp/master
cp -r * /tmp/master/
rm -fR /tmp/master/src/course
rm -f /tmp/master/src/templates/templates.js
rm -f /tmp/master/src/less/adapt.less
rm -fR /tmp/master/build

