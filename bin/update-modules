#!/bin/bash
# USAGE ./update-modules
for i in `seq 1 14`;
do
	cp .gitignore_module .gitignore
	git --git-dir=.module checkout module$i
	git --git-dir=.module pull
done
cp .gitignore_module .gitignore
git --git-dir=.module checkout navigation
git --git-dir=.module pull
cp .gitignore_module .gitignore
git --git-dir=.module checkout welcome
git --git-dir=.module pull
